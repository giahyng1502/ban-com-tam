<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RiceFood"
        tools:targetApi="31">
        <activity
            android:name=".Screen.CartScreen"
            android:exported="false"
            android:label="@string/title_activity_cart_screen"
            android:theme="@style/Theme.RiceFood" />
        <activity
            android:name=".Screen.MainScreen"
            android:exported="false"
            android:label="@string/title_activity_main_screen"
            android:theme="@style/Theme.RiceFood" />
        <activity
            android:name=".ButtomNavigation"
            android:exported="false"
            android:label="@string/title_activity_buttom_navigation"
            android:theme="@style/Theme.RiceFood" />
        <activity
            android:name=".MainActivity"
            android:exported="false"
            android:label="@string/title_activity_register_and_login"
            android:theme="@style/Theme.RiceFood" />
        <activity
            android:name=".MainControl"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.RiceFood">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>